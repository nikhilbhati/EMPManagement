package com.example.EmpManagmentBack.SERVICE;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;

import com.example.EmpManagmentBack.Model.Employee;
import com.example.EmpManagmentBack.Model.Team;
import com.example.EmpManagmentBack.REPO.EmpRepo;
import com.example.EmpManagmentBack.REPO.TeamRepo;

public class TeamService {

	@Autowired
	private TeamRepo TeamRepository;
	
	// fetching all employees
	public List<Team> getAllTeam(){
		List<Team> emps = (List<Team>)TeamRepository.findAll(); 
		return emps;
	}
}
